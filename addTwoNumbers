class Solution:
    def addTwoNumbers(self,l1,l2):
        d=ListNode(0)
        c=d
        carry=0
        while l1 or l2 or carry:
            x=l1.val if l1 else 0
            y=l2.val if l2 else 0
            total=x+y+carry
            carry=total//10
            digit=total%10
            c.next=ListNode(digit)
            c=c.next
            if l1:
                l1=l1.next
            if l2:
                l2=l2.next
        return d.next



